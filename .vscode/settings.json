{
    "remote.containers.copyGitConfig": false,
    "remote.containers.gitCredentialHelperConfigLocation": "none",
    "[c]": {
        "files.encoding": "macroman",
        "editor.tabSize": 4,
        "editor.insertSpaces": false,
        "files.eol": "auto",
    },
    "files.associations": {
        "functional": "c",
        "mbwindow.h": "c",
        "mbconstants.h": "c",
        "mbsessionlist.h": "c",
        "typeinfo": "cpp",
        "mbwsplash.h": "c",
        "optional": "c",
        "istream": "c",
        "ostream": "c",
        "system_error": "c",
        "array": "c",
        "tuple": "c",
        "type_traits": "c",
        "utility": "c",
        "mbdatamanager.h": "c",
        "mbtypes.h": "c",
        "string": "cpp",
        "cstring": "c"
    },
    "c-cpp-flylint.cppcheck.extraArgs": [
        "--std=c89",
        // Don't use --enable=all here because unusedFunction needs the whole project to be scanned and I'm not sure how to do that right now
        "--enable=warning,style,performance,portability,information",
    ],
    "c-cpp-flylint.lizard.enable": false,
    "c-cpp-flylint.flexelint.enable": false,
    "c-cpp-flylint.clang.enable": false,
    "c-cpp-flylint.clang.standard": [
        "c89"
    ],
    "c-cpp-flylint.flawfinder.enable": false,
    "c-cpp-flylint.cppcheck.includePaths": [
        "macbrew-ui/"
    ],
    "c-cpp-flylint.cppcheck.platform": "unix32"
}
